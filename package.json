{
  "name": "e2e-agent",
  "version": "1.0.0",
  "description": "An agent for generating end-to-end tests from a given URL",
  "license": "(MIT OR Apache-2.0)",
  "private": true,
  "author": {
    "name": "Brewen Couaran",
    "email": "brewcoua@brewen.dev",
    "url": "https://brewen.dev"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brewcoua/e2e-agent.git"
  },
  "homepage": "https://github.com/brewcoua/e2e-agent#readme",
  "bugs": {
    "url": "https://github.com/brewcoua/e2e-agent/issues",
    "email": "contact@brewen.dev"
  },
  "engines": {
    "node": ">=20.13.0",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "start": "dotenvx run -- bun ./src/index.ts | pino-pretty -i service",
    "build": "bun build ./src/index.ts --target=bun --outfile ./dist/e2e-agent.js --external=playwright --external=openai --minify",
    "format": "prettier --write ./src/**/*.ts"
  },
  "devDependencies": {
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@andreekeberg/imagedata": "^1.0.2",
    "@dotenvx/dotenvx": "^0.39.0",
    "deepmerge": "^4.3.1",
    "env-var": "^7.4.2",
    "extract-colors": "^4.0.6",
    "openai": "^4.47.1",
    "oxide.ts": "^1.1.0",
    "pino": "^9.1.0",
    "playwright": "^1.44.0"
  }
}