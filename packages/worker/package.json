{
    "name": "@webwisp/worker",
    "version": "1.0.0",
    "description": "A worker for the WebWisp autonomous agent.",
    "license": "(MIT OR Apache-2.0)",
    "main": "./dist/main.js",
    "author": {
        "name": "Brewen Couaran",
        "email": "brewcoua@brewen.dev",
        "url": "https://brewen.dev"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh@github.com:brewcoua/webwisp.git"
    },
    "homepage": "https://github.com/brewcoua/webwisp/tree/master/packages/orchestrator#readme",
    "bugs": {
        "url": "https://github.com/brewcoua/webwisp/issues",
        "email": "contact@brewen.dev"
    },
    "engines": {
        "node": ">=20.14.0",
        "npm": ">=10.5.0"
    },
    "scripts": {
        "start": "npm run build && npm run start:prod",
        "start:bulk": "npm run build && concurrently \"npm run start:prod\" \"npm run start:prod\" \"npm run start:prod\"",
        "start:prod": "node dist/main.js",
        "build": "npx rollup -c",
        "build:watch": "npx rollup -c -w",
        "format": "npx prettier --write ./src/**/*.ts"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.5",
        "chalk": "^5.3.0",
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "amqplib": "^0.10.4",
        "openai": "^4.51.0",
        "playwright": "^1.44.1",
        "winston": "^3.13.0"
    }
}
